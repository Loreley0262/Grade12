<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Showbooks">
   <children>
      <TableView prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="colMemberName" minWidth="150.0" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="colMemberId" minWidth="150.0" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="colBook" minWidth="150.0" prefWidth="75.0" text="Book" />
            <TableColumn fx:id="colBookId" minWidth="150.0" prefWidth="75.0" text="Book ID" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
