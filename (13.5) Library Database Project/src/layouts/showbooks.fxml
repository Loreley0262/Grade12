<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="layouts.Showbooks">
   <children>
      <TableView fx:id="bookTable" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="colBook" minWidth="300.0" prefWidth="75.0" text="Book" />
            <TableColumn fx:id="colId" minWidth="300.0" prefWidth="75.0" text="Book ID" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
